{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "maple",
    "name": "ParleyAI Documentations",
    "colors": {
        "primary": "#818cf9",
        "light": "#818cf9",
        "dark": "#818cf9"
    },
    "icons": {
        "library": "lucide"
    },
    "favicon": "/favicon.ico",
    "navigation": {
        "dropdowns": [
            {
                "dropdown": "Documentation",
                "icon": "book-open",
                "pages": [
                    "index",
                    "quickstart",
                    "development"
                ]
            },
            {
                "dropdown": "Changelog",
                "icon": "book-open",
                "pages": [
                    "changelog"
                ]
            }
        ],
        "global": {
            "anchors": [
                {
                    "anchor": "Blog",
                    "href": "https://parleyai.net/blog",
                    "icon": "newspaper"
                }
            ]
        }
    },
    "logo": {
        "light": "/logo-long.png",
        "dark": "/logo-long.png"
    },
    "navbar": {
        "links": [
            {
                "label": "Support",
                "href": "mailto:support@parleyai.net"
            }
        ],
        "primary": {
            "type": "button",
            "label": "Dashboard",
            "href": "https://parleyai.net/dashboard"
        }
    },
    "footer": {
        "socials": {
            "x": "https://x.com/parley_ai",
            "github": "https://github.com/ParleyAI"
        }
    }
}